              <string name="Source"><![CDATA[-- Compiled with roblox-ts v3.0.0-dev-9165cde
local TS = require(game:GetService("ReplicatedStorage"):WaitForChild("rbxts_include"):WaitForChild("RuntimeLib"))
local Reflect = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@flamework", "core", "out").Reflect
local Controller = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@flamework", "core", "out").Controller
local _services = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@rbxts", "services")
local Players = _services.Players
local RunService = _services.RunService
local UserInputService = _services.UserInputService
local Workspace = _services.Workspace
local CameraEffect = {
	None = "None",
	Shake = "Shake",
	Blur = "Blur",
	Desaturate = "Desaturate",
	Paranoia = "Paranoia",
	Death = "Death",
	Bobbing = "Bobbing",
}
local CameraController
do
	CameraController = setmetatable({}, {
		__tostring = function()
			return "CameraController"
		end,
	})
	CameraController.__index = CameraController
	function CameraController.new(...)
		local self = setmetatable({}, CameraController)
		return self:constructor(...) or self
	end
	function CameraController:constructor()
		self.activeEffects = {}
		self.connections = {}
		self.bobbingEnabled = true
		self.bobbingIntensity = 0.5
		self.bobbingSpeed = 1
		self.bobbingOffset = {
			x = 0,
			y = 0,
		}
		self.stepTime = 0
		self.headOffset = Vector3.new(0, 0.25, 0)
		self.mouseSensitivity = 0.005
		self.cameraAngleX = 0
		self.cameraAngleY = 0
		self.maxCameraAngleY = math.rad(80)
		self.camera = Workspace.CurrentCamera
		self:setupFirstPersonCamera()
		self:startCameraUpdate()
		self:setupMouseInput()
	end
	function CameraController:setupFirstPersonCamera()
		local player = Players.LocalPlayer
		-- Wait for character
		if player.Character then
			self:setupCharacterReferences(player.Character)
		end
		-- Listen for character respawning
		local characterConnection = player.CharacterAdded:Connect(function(character)
			self:setupCharacterReferences(character)
		end)
		local _exp = self.connections
		table.insert(_exp, characterConnection)
		-- Set camera to first person
		self.camera.CameraType = Enum.CameraType.Scriptable
		-- Hide mouse cursor and lock it
		UserInputService.MouseBehavior = Enum.MouseBehavior.LockCenter
		UserInputService.MouseIconEnabled = false
	end
	function CameraController:setupCharacterReferences(character)
		self.character = character
		self.humanoid = character:WaitForChild("Humanoid")
		self.rootPart = character:WaitForChild("HumanoidRootPart")
		self.head = character:WaitForChild("Head")
		self.humanoid.AutoRotate = false
		-- Destroy accessories on head to prevent clipping
		local _exp = self.character:GetChildren()
		-- ▼ ReadonlyArray.forEach ▼
		local _callback = function(child)
			if child:IsA("Accessory") then
				child:Destroy()
			end
		end
		for _k, _v in _exp do
			_callback(_v, _k - 1, _exp)
		end
		-- ▲ ReadonlyArray.forEach ▲
	end
	function CameraController:setupMouseInput()
		local mouseConnection = UserInputService.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				local delta = input.Delta
				self.cameraAngleX -= delta.X * self.mouseSensitivity
				self.cameraAngleY = math.clamp(self.cameraAngleY - delta.Y * self.mouseSensitivity, -self.maxCameraAngleY, self.maxCameraAngleY)
				self:updateCharacterRotation()
			end
		end)
		local _exp = self.connections
		table.insert(_exp, mouseConnection)
	end
	function CameraController:updateCharacterRotation()
		if not self.rootPart then
			return nil
		end
		-- Create a CFrame that only rotates around the Y-axis (horizontal rotation)
		local characterRotation = CFrame.Angles(0, self.cameraAngleX, 0)
		-- Apply rotation to the character's root part while preserving position
		local currentPosition = self.rootPart.CFrame.Position
		self.rootPart.CFrame = CFrame.new(currentPosition) * characterRotation
	end
	function CameraController:startCameraUpdate()
		local renderConnection = RunService.RenderStepped:Connect(function(deltaTime)
			self:updateCamera(deltaTime)
		end)
		local _exp = self.connections
		table.insert(_exp, renderConnection)
	end
	function CameraController:updateCamera(deltaTime)
		if not self.character or not self.humanoid or not self.rootPart or not self.head then
			return nil
		end
		-- Position camera at head position with offset
		local headPosition = self.head.CFrame.Position
		local _headOffset = self.headOffset
		local cameraPosition = headPosition + _headOffset
		-- Apply camera bobbing if enabled and moving
		local _condition = self.bobbingEnabled
		if _condition then
			local _activeEffects = self.activeEffects
			local _bobbing = CameraEffect.Bobbing
			_condition = _activeEffects[_bobbing] ~= nil
		end
		if _condition then
			local walkSpeed = self.humanoid.WalkSpeed
			local moveVector = self.humanoid.MoveDirection
			local isMoving = moveVector.Magnitude > 0.1
			if isMoving and walkSpeed > 0 then
				self.stepTime += deltaTime * self.bobbingSpeed * (walkSpeed / 16)
				-- Calculate bobbing offset
				local bobbingX = math.sin(self.stepTime * 2) * self.bobbingIntensity * 0.1
				local bobbingY = math.abs(math.sin(self.stepTime * 4)) * self.bobbingIntensity * 0.15
				self.bobbingOffset = {
					x = bobbingX,
					y = bobbingY,
				}
			else
				-- Smoothly return to center when not moving
				self.bobbingOffset.x = self.bobbingOffset.x * 0.9
				self.bobbingOffset.y = self.bobbingOffset.y * 0.9
			end
			-- Apply bobbing to camera position
			local bobbingVector = Vector3.new(self.bobbingOffset.x, self.bobbingOffset.y, 0)
			cameraPosition = cameraPosition + bobbingVector
		end
		-- Create camera CFrame with proper rotation based on mouse input
		local _exp = CFrame.Angles(0, self.cameraAngleX, 0)
		local _arg0 = CFrame.Angles(self.cameraAngleY, 0, 0)
		local rotationCFrame = _exp * _arg0
		self.camera.CFrame = CFrame.new(cameraPosition) * rotationCFrame
	end
	function CameraController:addEffect(effect)
		local _activeEffects = self.activeEffects
		local _effect = effect
		_activeEffects[_effect] = true
		if effect == CameraEffect.Bobbing then
			self.bobbingEnabled = true
		end
	end
	function CameraController:removeEffect(effect)
		local _activeEffects = self.activeEffects
		local _effect = effect
		_activeEffects[_effect] = nil
		if effect == CameraEffect.Bobbing then
			self.bobbingEnabled = false
			self.bobbingOffset = {
				x = 0,
				y = 0,
			}
		end
	end
	function CameraController:hasEffect(effect)
		local _activeEffects = self.activeEffects
		local _effect = effect
		return _activeEffects[_effect] ~= nil
	end
	function CameraController:isFirstPerson()
		return true
	end
	function CameraController:setBobbingIntensity(intensity)
		self.bobbingIntensity = math.clamp(intensity, 0, 2)
	end
	function CameraController:setBobbingSpeed(speed)
		self.bobbingSpeed = math.clamp(speed, 0.1, 3)
	end
	function CameraController:setMouseSensitivity(sensitivity)
		self.mouseSensitivity = math.clamp(sensitivity, 0.001, 0.02)
	end
	function CameraController:enableBobbing()
		self:addEffect(CameraEffect.Bobbing)
	end
	function CameraController:disableBobbing()
		self:removeEffect(CameraEffect.Bobbing)
	end
	function CameraController:cleanup()
		-- Disconnect all connections
		for _, connection in self.connections do
			connection:Disconnect()
		end
		self.connections = {}
		-- Restore default camera settings
		self.camera.CameraType = Enum.CameraType.Custom
		UserInputService.MouseBehavior = Enum.MouseBehavior.Default
		UserInputService.MouseIconEnabled = true
		-- Restore head visibility
		if self.head then
			self.head.Transparency = 0
			for _, child in self.head:GetChildren() do
				if child:IsA("Decal") or child:IsA("Texture") then
					child.Transparency = 0
				end
			end
		end
	end
	do
		-- (Flamework) CameraController metadata
		Reflect.defineMetadata(CameraController, "identifier", "client/controllers/camera@CameraController")
	end
end
-- (Flamework) CameraController decorators
Reflect.decorate(CameraController, "$:flamework@Controller", Controller, {})
return {
	CameraEffect = CameraEffect,
	CameraController = CameraController,
}
