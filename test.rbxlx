              <string name="Source"><![CDATA[-- Compiled with roblox-ts v3.0.0-dev-9165cde
local TS = require(game:GetService("ReplicatedStorage"):WaitForChild("rbxts_include"):WaitForChild("RuntimeLib"))
local Reflect = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@flamework", "core", "out").Reflect
local UserInputService = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@rbxts", "services").UserInputService
local CameraEffect = TS.import(script, script.Parent, "camera").CameraEffect
local Controller = TS.import(script, game:GetService("ReplicatedStorage"), "rbxts_include", "node_modules", "@flamework", "core", "out").Controller
local GameController
do
	GameController = setmetatable({}, {
		__tostring = function()
			return "GameController"
		end,
	})
	GameController.__index = GameController
	function GameController.new(...)
		local self = setmetatable({}, GameController)
		return self:constructor(...) or self
	end
	function GameController:constructor(cameraController)
		self:setupRemotes()
		self:setupInput()
		self.cameraController = cameraController
		print("GameController initialized with first-person camera")
	end
	function GameController:setupRemotes()
		-- TODO: Set up network events
	end
	function GameController:setupInput()
		UserInputService.InputBegan:Connect(function(input)
			if input.KeyCode == Enum.KeyCode.E then
				self:handleInteraction()
			elseif input.KeyCode == Enum.KeyCode.C then
				-- Toggle camera shake effect as example
				self:toggleCameraEffect()
			end
		end)
	end
	function GameController:toggleCameraEffect()
		-- Example: Toggle shake effect
		if self.cameraController:hasEffect(CameraEffect.Shake) then
			self.cameraController:removeEffect(CameraEffect.Shake)
			print("Removed camera shake")
		else
			self.cameraController:addEffect(CameraEffect.Shake)
			print("Added camera shake")
		end
	end
	function GameController:onGameStateChanged(gameState)
		-- Update UI based on game state
		print(`Game phase changed to: {gameState.currentPhase}`)
	end
	function GameController:onPlayerKilled(victim, killer)
		-- Handle visual effects, sounds, etc.
		print(`{victim.Name} was killed!`)
	end
	function GameController:handleInteraction()
		-- Handle player interaction logic
		print("Player interacted with an object.")
	end
	function GameController:addCameraEffect(effect)
		self.cameraController:addEffect(effect)
	end
	function GameController:removeCameraEffect(effect)
		self.cameraController:removeEffect(effect)
	end
	function GameController:cleanup()
		self.cameraController:cleanup()
	end
	do
		-- (Flamework) GameController metadata
		Reflect.defineMetadata(GameController, "identifier", "client/controllers/game@GameController")
		Reflect.defineMetadata(GameController, "flamework:parameters", { "client/controllers/camera@CameraController" })
	end
end
-- (Flamework) GameController decorators
Reflect.decorate(GameController, "$:flamework@Controller", Controller, {})
return {
	GameController = GameController,
}
]]></string>
